<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.dev.dina.proj.client.resources.ProjectResources"
		field="res" />

	<g:FlowPanel styleName="{res.css.answerPanel}">
		<g:FlowPanel styleName="{res.css.answersContainer}">
			<g:TextBox ui:field="firstValue" styleName="{res.css.answer}" />
			<g:TextBox ui:field="secondValue" styleName="{res.css.answer}" />
			<g:TextBox ui:field="thirdValue" styleName="{res.css.answer}" />
			<g:TextBox ui:field="forthValue" styleName="{res.css.answer}" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 